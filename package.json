{
	"name": "xanovi",
	"description": "Xanovi est une application moderne Node.js qui centralise les librairies de Literate. Elle ajoute également d'autres fonctionnalités peu utilisées mais présentes sur les différentes plateformes",
	"authors": "Mewax07",
	"version": "1.0.0",
	"sideEffects": false,
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"workspaces": [
		"pawnote",
		"meowdle",
		"utils"
	],
	"scripts": {
		"build": "bunup",
		"e:instance": "bun run ./examples/pawnote/instance.ts -- --p:dev2",
		"e:p:auth": "bun run ./examples/pawnote/parents/auth.ts -- --p:dev2",
		"e:p:children": "bun run ./examples/pawnote/parents/children.ts -- --p:dev2",
		"e:s:auth": "bun run ./examples/pawnote/students/auth.ts -- --p:dev2",
		"e:s:homepage": "bun run ./examples/pawnote/students/homepage.ts -- --p:dev2",
		"e:s:homework": "bun run ./examples/pawnote/students/homework.ts -- --p:dev1",
		"e:s:timetable": "bun run ./examples/pawnote/students/timetable.ts -- --p:dev2"
	},
	"type": "module",
	"private": true,
	"devDependencies": {
		"prettier": "3.6.2",
		"prettier-plugin-organize-imports": "4.3.0",
		"bunup": "0.16.10",
		"@types/node": "24.10.0",
		"@types/bun": "1.3.2"
	},
	"peerDependencies": {
		"typescript": "5.9.3"
	},
	"dependencies": {
		"@noble/ciphers": "2.0.1",
		"@noble/hashes": "2.0.1",
		"@scure/base": "2.0.0",
		"fflate": "0.8.2",
		"micro-rsa-dsa-dh": "0.2.3",
		"schwi": "0.8.0"
	}
}
